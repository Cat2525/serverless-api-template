{
  "name": "serverless-api-typescript",
  "version": "2.0.1",
  "description": "Serverless API example using Typescript",
  "scripts": {
    "deploy": "sls deploy",
    "remove": "sls remove",
    "test": "jest --coverage",
    "watch-tests": "jest --watch",
    "e2e-test": "jest -c=jest.config.e2e.js",
    "load-data": "sls invoke -f load-messages-snapshot",
    "watch-docs": "live-server docs & watch 'npm run generate-docs' app",
    "generate-docs": "swagger-jsdoc -d docs/swagger-spec.yml -o docs/swagger.json",
    "codecov": "codecov -f coverage/*.json"
  },
  "dependencies": {
    "@manwaring/lambda-wrapper": "^1.2.2",
    "source-map-support": "^0.5.16",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-typescript": "^7.7.2",
    "@serverless/event-mocks": "^1.1.1",
    "@types/aws-lambda": "^8.10.36",
    "@types/chance": "^1.0.7",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.8",
    "@types/request-promise-native": "^1.0.17",
    "@types/uuid": "^3.4.6",
    "babel-jest": "^24.9.0",
    "chance": "^1.1.3",
    "codecov": "^3.6.1",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "live-server": "^1.2.1",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.8",
    "rimraf": "^3.0.0",
    "serverless": "^1.57.0",
    "serverless-cloudformation-resource-counter": "^1.0.2",
    "serverless-lumigo": "^1.6.0",
    "serverless-plugin-iam-checker": "^1.0.1",
    "serverless-plugin-test-helper": "^1.3.1",
    "serverless-prune-plugin": "^1.4.0",
    "serverless-webpack": "^5.2.0",
    "swagger-jsdoc": "^3.4.0",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.2",
    "watch": "^1.0.2",
    "webpack": "^4.41.2"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run generate-docs"
    }
  },
